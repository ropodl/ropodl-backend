{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "minLength": 3,
      "maxLength": 255,
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true,
      "minLength": 3,
      "regex": "/(\\b(?:a|am|is|are|was|were|be|and|been|being|has|have|had)\\b)|([^A-Z0-9-]+)|(^-+)|(-+$)/g"
    },
    "excerpt": {
      "type": "string",
      "minLength": 3,
      "maxLength": 255,
      "required": true
    },
    "content": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml",
        "maxLengthWords": null
      },
      "required": true
    },
    "featured_image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    },
    "open_graph": {
      "type": "component",
      "component": "shared.open-graph",
      "repeatable": false
    },
    "seo": {
      "type": "component",
      "component": "shared.seo",
      "repeatable": false
    }
  }
}
