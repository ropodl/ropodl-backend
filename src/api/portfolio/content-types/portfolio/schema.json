{
  "kind": "collectionType",
  "collectionName": "portfolios",
  "info": {
    "singularName": "portfolio",
    "pluralName": "portfolios",
    "displayName": "Portfolio"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true,
      "regex": "/(\\b(?:a|am|is|are|was|were|be|and|been|being|has|have|had)\\b)|([^A-Z0-9-]+)|(^-+)|(-+$)/g"
    },
    "content": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    },
    "portfolio_type": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::type.type"
    }
  }
}
